description: \"\"});\n        setSelectedMovie(null)\n    }\n\n    const movieCreated = movie =>{\n        const newMovies = [...movies, movie]\n        setMovies(newMovies);\n        // setEditedMovie(null)\n        // selectedMovie(null)\n    }\n\n\n    const removeClicked = movie => {\n        const newMovies = movies.filter(mov => mov.id !== movie.id)\n        setMovies(newMovies);\n    }\n\n\n    const logoutUser = () => {\n        removeCookie(['mr-token'], {path: \"/\"});\n    }\n\n    if (loading) return <h1> Loading...</h1>\n    if (error) return <h1> Error Loading movies</h1>\n\n    return (\n        <div className=\"App\">\n            <header>\n                <h1>\n                    <FontAwesomeIcon icon={faFilm} />\n                    <span>Movie Rater</span>\n                </h1>\n                <FontAwesomeIcon icon={faSignOutAlt} id=\"fa-sing-out-alt\" onClick={logoutUser}/>\n            </header>\n            <div className=\"layout\">\n                <div>\n                    <MovieList\n                        movies={movies}\n                        movieClicked={loadMovie}\n                        editClicked={editClicked}\n                        removeClicked={removeClicked}\n                    />\n                    <button onClick={newMovie}> New Movie </button>\n                </div>\n                {selectedMovie ? <MovieDetails movie={selectedMovie} updateMovie={loadMovie}/>:null}\n                {editedMovie ? <MovieForm movie={editedMovie} updateMovie={updatedMovie} movieCreated={movieCreated}>\n\n                </MovieForm>:null}\n            </div>\n        </div>\n  );\n}\n\nexport default App;\n","/home/weslleylc/WebstormProjects/movie_rater/src/components/movie-list.js",[],"/home/weslleylc/WebstormProjects/movie_rater/src/components/movie-details.js",[],"/home/weslleylc/WebstormProjects/movie_rater/src/components/movie-form.js",[],"/home/weslleylc/WebstormProjects/movie_rater/src/api-service.js",[],"/home/weslleylc/WebstormProjects/movie_rater/src/components/auth.js",[],"/home/weslleylc/WebstormProjects/movie_rater/src/hooks/useFetch.js",["98"],"import {useState, useEffect} from \"react\";\nimport {API} from \"../api-service\";\nimport {useCookies} from \"react-cookie\";\n\nfunction useFetch() {\n    const [data, setData] = useState([]);\n    const [loading, setLoading] = useState(true)\n    const [error, setError] = useState(true)\n    const [cookies] = useCookies(['mr-token'])\n\n    useEffect(() =>{\n        async function fetchData() {\n            setLoading(true);\n            setError();\n            const data = await API.getMovies(cookies['mr-token']).catch(err => setError(err))\n            setData(data)\n            setLoading(false)\n        }\n        fetchData();\n    },[])\n    return [data, loading, error]\n}\nexport {useFetch};","/home/weslleylc/PycharmProjects/instagram/frontend/src/index.js",[],["99","100"],"/home/weslleylc/PycharmProjects/instagram/frontend/src/App.js",["101","102","103","104","105"],"import React, { useState, useEffect} from \"react\";\nimport {useCookies} from \"react-cookie\";\nimport PostsLists from \"./components/PostsLists/PostsLists\";\nimport NavBar from \"./components/NavBar/NavBar\";\nimport './App.scss';\nimport {API} from \"./api-service\";\nimport CreateInstagramPost from \"./components/CreateIntagramPost/CreateInstagramPost\";\nimport Wrapper from \"./components/Wrapper/Wrapper\"\n\n\nfunction App() {\n    const [posts, setPosts] = useState([]);\n    const [cookies, setCookie, removeCookie] = useCookies(['mr-token'])\n    const [page, setPage] = useState(1);\n    const [maxPage, setMaxPage] = useState(1);\n    const [create, setCreate] = useState(false);\n\n\n    async function fetchData() {\n        const data = await API.getPosts(cookies['mr-token'], page)\n        setPosts(data.data);\n        setMaxPage(data.meta.last_page);\n    }\n\n    useEffect(() =>{\n        fetchData();\n    },[, page])\n\n    useEffect(() => {\n        if (!cookies['mr-token']) window.location.href = '/';\n        if (cookies['mr-token'] === 'undefined') logoutUser();\n    }, [cookies])\n\n\n    const createPost = (quantity, link) =>{\n        async function fetchDataComments() {\n            const post = await API.createPost(cookies['mr-token'], {\"number_comments\": quantity, \"link\": link})\n            fetchData();\n            // setPosts([...posts, post])\n            setCreate(false)\n        }\n        fetchDataComments();\n    }\n\n    const deletePost = (id) => {\n        async function deletePostById() {\n            return await API.deletePost(cookies['mr-token'], id)\n        }\n        deletePostById().then(() => setPosts(posts.filter(post => post.id !== id)));\n    }\n\n\n    const logoutUser = () => {\n        removeCookie(['mr-token'], {path: \"/\"});\n    }\n\n    return (\n        <div>\n            <NavBar logOut={logoutUser} setCreate={setCreate} />\n            <Wrapper>\n                {create?\n                    <CreateInstagramPost createPost={createPost}/>:\n                    <div>\n                        <PostsLists  posts={posts} cookies={cookies} deletePost={deletePost}\n                                     page={page} setPage={setPage} maxPage={maxPage}/>\n                    </div>\n                }\n            </Wrapper>\n        </div>\n\n\n    );\n}\n\nexport default App;\n","/home/weslleylc/PycharmProjects/instagram/frontend/src/reportWebVitals.js",[],"/home/weslleylc/PycharmProjects/instagram/frontend/src/components/Auth/Auth.js",[],"/home/weslleylc/PycharmProjects/instagram/frontend/src/api-service.js",[],"/home/weslleylc/PycharmProjects/instagram/frontend/src/components/PostsLists/PostsLists.js",[],"/home/weslleylc/PycharmProjects/instagram/frontend/src/components/NavBar/NavBar.js",["106","107","108","109"],"import React, { useState } from 'react';\nimport PropTypes from \"prop-types\";\nimport {\n    Collapse,\n    Navbar,\n    NavbarToggler,\n    NavbarBrand,\n  